= render "simple_search", list_path: expense_details_path, new_path: new_v2_expense_details_path

= form_tag multi_ops_expense_details_path do 
  %table{id: "Expense_List", class: "table table-striped"} 
    %thead
      %tr
        %th
        %th Date
        %th Expense Type
        %th Payment Type
        %th Amout
        %th Comment
        %th Operation
    %tbody
      - expense_details.each do |expense_detail|
        = render "expense_details/expense_detail", expense_detail: expense_detail

  = render "set_tbl_header", list: expense_details, src_file: "/assets/ui/list_to_table_expense.js"
  = render "multi_ops"

%br
#Expense_Detail_Form_new{class: "btn"}
  New

#Expense_Detail_Form  
  = render "expense_details/form"

%script{:type => "text/javascript",
  :src  => "/assets/expense_details.js"}   

  
